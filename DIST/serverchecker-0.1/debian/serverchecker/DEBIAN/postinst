#!/bin/sh
set -e
# Automatically added by dh_installinit
if [ "$1" = "configure" ] || [ "$1" = "abort-upgrade" ]; then
	if [ -x "/etc/init.d/serverchecker" ]; then
		update-rc.d serverchecker defaults >/dev/null
	fi
	if [ -x "/etc/init.d/serverchecker" ] || [ -e "/etc/init/serverchecker.conf" ]; then
		invoke-rc.d serverchecker start || exit $?
	fi
fi
# End automatically added section
